 .../modules/uihandler/Bundle_nb.properties       | 16 ++++++++--------
 1 file changed, 8 insertions(+), 8 deletions(-)

diff --git a/nb/ide.branding/uihandler/src/org/netbeans/modules/uihandler/Bundle_nb.properties b/nb/ide.branding/uihandler/src/org/netbeans/modules/uihandler/Bundle_nb.properties
index 6877083360..cd9c183ec3 100644
--- a/nb/ide.branding/uihandler/src/org/netbeans/modules/uihandler/Bundle_nb.properties
+++ b/nb/ide.branding/uihandler/src/org/netbeans/modules/uihandler/Bundle_nb.properties
@@ -34,17 +34,17 @@ ApplicationVersionSysPropertyResourceArg=netbeans.buildnumber
 
 # URL to display to as a welcome text
 #NOI18N
-WELCOME_URL=https://netbeans.org/nonav/uigestures/index3.html
+WELCOME_URL=http://www.openbeans.org/uigestures/welcome.html
 
 #NOI18N
-METRICS_URL=https://netbeans.org/nonav/uigestures/index4.html
+METRICS_URL=http://www.openbeans.org/uigestures/metrics.html
 
 # URL to display to as a welcome text in case of error
 #NOI18N
-ERROR_URL=https://netbeans.apache.org/nb/issues.html
+ERROR_URL=http://www.openbeans.org/uigestures/error.html
 
 #NOI18N
-CHECKING_SERVER_URL=http://statistics.netbeans.org/analytics/authenticate?username={0}&secur_passwd={1}
+CHECKING_SERVER_URL=http://www.openbeans.org/analytics/authenticate?username={0}&secur_passwd={1}
 LOG_FILE=var/log/messages.log
 # {0} - host
 # {1} - url of upload
@@ -55,7 +55,7 @@ MSG_ConnetionFailed=Upload of log to {0} failed!
 # {2} - the absolute path to the IDE's log file (defined by the LOG_FILE key above)
 # String between [URL] and [/URL] is rendered as an URL and it opens in a browser upon a click
 # String between [TTC] and [/TTC] (Text To Copy) is possible to select and copy
-MSG_ConnetionFailedReport=Upload of log failed, please report the original problem directly at [URL]http://www.netbeans.org/community/issues.html[/URL] and attach messages.log file.\
+MSG_ConnetionFailedReport=Upload of log failed, please report the original problem directly at [URL]http://www.openbeans.org[/URL] and attach messages.log file.\
 \nThe messages.log file is located at [TTC]{2}[/TTC].
 # {0} - host
 # {1} - url of upload
@@ -68,19 +68,19 @@ MSG_ConnetionFailedWithResponse=Upload of log to {0} failed: {3}
 # {3} - response message
 # String between [URL] and [/URL] is rendered as an URL and it opens in a browser upon a click
 # String between [TTC] and [/TTC] (Text To Copy) is possible to select and copy
-MSG_ConnetionFailedReportWithResponse=Upload of log failed ({3}), please report the original problem directly at [URL]http://www.netbeans.org/community/issues.html[/URL] and attach messages.log file.\
+MSG_ConnetionFailedReportWithResponse=Upload of log failed ({3}), please report the original problem directly at [URL]http://www.openbeans.org[/URL] and attach messages.log file.\
 \nThe messages.log file is located at [TTC]{2}[/TTC].
 
